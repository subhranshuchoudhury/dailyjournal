<%- include('partials/header') -%>
<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input type="text" name="postTitle" class="form-control"  placeholder="title here..">
    </div>
    
    
    <div class="form-group">
      <label>Post Content</label>
      <textarea class="form-control" name="postContent" rows="3" placeholder="content here..."></textarea>
    </div>
  <button type="submit" class="btn btn-primary">Publish</button>
  </form>


<%- include('partials/footer') -%>